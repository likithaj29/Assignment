package com.capgemini.prgm1;

import java.util.Scanner;

public class FibonacciSeries {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter length");
		int len=sc.nextInt();
		int a1=0;
		int a2=1;
		for(int i=0;i<=len;i++) {
			System.out.println(a1+", ");
			int sum= a1+a2;
			a1=a2;
			a2=sum;
		}
	}

}
